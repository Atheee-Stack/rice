    "@nx/eslint-plugin": "21.1.3",
    "@nx/jest": "21.1.3",
    "@nx/js": "21.1.3",
    "@nx/nest": "^21.1.3",
    "@nx/rollup": "21.1.3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.1.3",
    "prettier": "^2.6.2",
